#include <stdio.h>

int main(){
	char c;
	printf("Nhap ki tu: ");
	scanf("%c",&c);
	if((c<'A')||(c>'Z'&&c<'a')||c>'z'){
		printf("Ki tu nhap khong hop le moi nhap lai\n");
	}
	else{
	if(c=='a'||c=='o'||c=='e'||c=='u'||c=='i'||c=='A'||c=='O'||c=='E'||c=='U'||c=='I'){
		printf("%c la mot nguyen am",c);
	}
	else{
		printf("%c la mot phu am",c);
	}
}
}
